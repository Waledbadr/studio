{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Install html2canvas",
			"type": "shell",
			"command": "npm",
			"args": [
				"install",
				"html2canvas"
			],
			"problemMatcher": [
				"$npm-stylish"
			],
			"group": "build"
		},
		{
			"label": "Run dev server",
			"type": "shell",
			"command": "npm",
			"args": [
				"run",
				"dev"
			],
			"isBackground": true,
			"problemMatcher": [
				"$tsc"
			],
			"group": "build"
		}
	]
}